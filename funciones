def add(numero1,numero2):
    return numero1+numero2

def substract(numero1,numero2):
    return numero1-numero2

def multply(numero1,numero2):
    return numero1*numero2

def divide(numero1,numero2):
    return numero1/numero2

print(add(8,9))
print(substract(5,9))
print(multply(-3,2))
print(divide(5,1))
